mapper.py

 import sys
for line in sys.stdin:
 line = line.strip()
 columns = line.split()
 if len(columns)==3:
 salary = columns[2]
 print(f"None\t{salary}")



reducer.py

import sys
max_salary = 0
for line in sys.stdin:
 key, value = line.strip().split('\t', 1)

 try:
 salary = int(value)
 except ValueError:
 continue

 if salary > max_salary:
 max_salary = salary
print(f"Max Salary: {max_salary}")
